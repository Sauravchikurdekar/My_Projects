<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="d" class="com.demo.Department">
		<property name="dId" value="101"></property>
		<property name="dName" value="Science"></property>
	</bean>
	<bean id="i" class="com.demo.Institute">
		<property name="instituteId" value="1004"></property>
		<property name="name" value="Seed"></property>
		<property name="dept" ref="d"></property>

	</bean>

</beans>